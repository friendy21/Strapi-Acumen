{
    "kind": "collectionType",
    "collectionName": "tags",
    "info": {
        "singularName": "tag",
        "pluralName": "tags",
        "displayName": "Tag",
        "description": "Article tags for filtering and organization"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "tenant_id": {
            "type": "string",
            "required": true,
            "configurable": false,
            "private": true
        },
        "name": {
            "type": "string",
            "required": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name",
            "required": true
        },
        "articles": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::article.article",
            "mappedBy": "tags"
        }
    }
}