{
    "extends": "@strapi/typescript-utils/tsconfigs/server",
    "compilerOptions": {
        "outDir": "dist",
        "rootDir": ".",
        "baseUrl": "./",
        "skipLibCheck": true,
        "paths": {
            "~/*": [
                "./*"
            ]
        }
    },
    "include": [
        "src/**/*",
        "config/**/*"
    ],
    "exclude": [
        "node_modules/",
        "build/",
        "dist/",
        ".cache/",
        ".tmp/"
    ]
}